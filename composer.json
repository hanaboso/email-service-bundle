{
    "name": "hanaboso/email-service-bundle",
    "description": "Hanaboso - email-service-bundle",
    "license": "Apache-2.0",
    "version": "1.3.0",
    "require": {
        "php": "^8.2",
        "ext-iconv": "*",
        "hanaboso/commons-bundle": "^2.9",
        "symfony/google-mailer": "^v6.2",
        "symfony/mailer": "^v6.2",
        "symfony/monolog-bundle": "^v3.8",
        "twig/twig": "^v3.6"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "EmailServiceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmailServiceBundleTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
