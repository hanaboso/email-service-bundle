{
    "name": "hanaboso/email-service-bundle",
    "description": "Hanaboso - email-service-bundle",
    "license": "Apache-2.0",
    "version": "1.5.0",
    "require": {
        "php": "^8.3",
        "ext-iconv": "*",
        "hanaboso/commons-bundle": "^2.11",
        "symfony/google-mailer": "^7.1",
        "symfony/mailer": "^7.1",
        "symfony/monolog-bundle": "^3.10",
        "twig/twig": "^3.10"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "EmailServiceBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EmailServiceBundleTests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
