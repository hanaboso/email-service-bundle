{
  "name": "hanaboso/email-service-bundle",
  "license": "proprietary",
  "autoload": {
    "psr-4": {
      "EmailServiceBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "sensio/framework-extra-bundle": "^5.1",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/monolog-bundle": "^3.1",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/templating": "^4.0",
    "symfony/twig-bundle": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "hanaboso/php-check-utils": "^1.0",
    "phpunit/phpunit": "^7.0"
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
